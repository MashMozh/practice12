try:
    # Открываем файл и читаем его содержимое
    with open('16', 'r', encoding='utf-8') as f:
        text = f.read()

    # Список для отслеживания открывающих скобок
    skobki = []

    # Перебираем символы в тексте
    for i in text:
        if i == '(':  # Если нашли открывающую скобку, добавляем ее в список
            skobki.append(i)
        elif i == ')':  # Если нашли закрывающую скобку
            if skobki:
                skobki.pop()  # Убираем открывающую скобку из списка
            else:
                # Если список пуст, значит, закрывающая скобка лишняя
                print("Неправильная расстановка скобок (лишняя закрывающая скобка).")
                break
    else:
        # Если в списке остались открывающие скобки, значит, они не закрыты
        if skobki:
            print("Неправильная расстановка скобок (лишняя открывающая скобка).")
        else:
            print("Скобки расставлены правильно.")

except FileNotFoundError:
    print("Файл не найден.")
